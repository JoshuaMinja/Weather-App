{
  "expo": {
    "name": "My City Weather",
    "slug": "weatherApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/weather.png",
    "splash": {
      "image": "./assets/weather.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reactnative.mycityweather",
      "buildNumber": "1.0.0"
    },
    "web": {
      "favicon": "./assets/weather.png"
    },
    "android": {
      "package": "com.reactnative.mycityweather",
      "versionCode": 1
    }
  }
}
